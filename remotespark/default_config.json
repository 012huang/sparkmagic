{
  "serialize": false,
  "serialize_periodically": false,
  "serialize_period_seconds": 3,
  "display_info": false,

  "use_altair": true,
  "default_chart_type": "area",

  "kernel_python_username": "",
  "kernel_python_password": "",
  "kernel_python_url": "",

  "kernel_scala_username": "",
  "kernel_scala_password": "",
  "kernel_scala_url": "",

  "logging_config": {
    "version": 1,
    "formatters": {
      "magicsFormatter": { 
        "format": "%(asctime)s\t%(levelname)s\t%(message)s",
        "datefmt": ""
      }
    },
    "handlers": {
      "magicsHandler": { 
        "class": "remotespark.livyclientlib.filehandler.MagicsFileHandler",
        "formatter": "magicsFormatter"
      }
    },
    "loggers": {
      "magicsLogger": { 
        "handlers": ["magicsHandler"],
        "level": "DEBUG"
      }
    }
  },

  "execute_timeout_seconds": 3600,
  "status_sleep_seconds": 2,
  "statement_sleep_seconds": 2,
  "create_sql_context_timeout_seconds": 60,

  "ignore_ssl_errors": false
}